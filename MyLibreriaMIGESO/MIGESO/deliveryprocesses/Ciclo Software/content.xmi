  <org.eclipse.epf.uma:DescriptorDescription xmi:id="-tE8pojxAGquWVsd-V_j3aw" name="developer-master,_9uWQccMJEeWhReE6cXG9Lw" guid="-tE8pojxAGquWVsd-V_j3aw">
    <refinedDescription>&lt;p>&#xD;
    Caracteristicas:&#xD;
&lt;/p>&#xD;
